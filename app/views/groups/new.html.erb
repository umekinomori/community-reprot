<% @group = Group.new unless @group %>
<%= form_for @group do |f| %>
  <div class="filed">
    <%= f.label :district %> <br />
    <%= f.select :district, 
    [["足立区","足立区"],
     ["荒川区","荒川区"],
     ["板橋区","板橋区"],
     ["江戸川区","江戸川区"],
     ["大田区","大田区"],
     ["葛飾区","葛飾区"],
     ["北区","北区"],
     ["江東区","江東区"],
     ["品川区","品川区"],
     ["渋谷区","渋谷区"],
     ["新宿区","新宿区"],
     ["杉並区","杉並区"],
     ["墨田区","墨田区"],
     ["世田谷区","世田谷区"],
     ["台東区","台東区"],
     ["千代田区","千代田区"],
     ["中央区","中央区"],
     ["豊島区","豊島区"],
     ["中野区","中野区"],
     ["練馬区","練馬区"],
     ["文京区","文京区"],
     ["港区","港区"],
     ["目黒区","目黒区"],]%>
  </div>
  <div class="action">
  <%= f.submit "登録" %>
  </div>
<% end %>