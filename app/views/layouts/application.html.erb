<!DOCTYPE html>
<html>
  <head>
    <title>CommunityReprot</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <header class="header">
      <div class="header__title">
        <%= link_to "CommunityReport", root_path, class: "header__title--text" %>
        <div class="header__title--edit">
          <i class="fas fa-edit"></i>
          <i class="fas fa-user-plus"></i>
        </div>
      </div>
    </header>
    <%= yield %>
  </body>
</html>
